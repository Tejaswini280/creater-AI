{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && npm run build:server",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "cross-env NODE_ENV=test SKIP_RATE_LIMIT=1 NODE_OPTIONS=--max_old_space_size=4096 jest --runInBand --coverage",
    "test:watch": "cross-env NODE_ENV=test SKIP_RATE_LIMIT=1 jest --watch",
    "test:server": "cross-env NODE_ENV=test SKIP_RATE_LIMIT=1 NODE_OPTIONS=--max_old_space_size=4096 jest --config jest.server.config.cjs --runInBand --coverage",
    "e2e": "cross-env NODE_ENV=test PLAYWRIGHT_JSON_OUTPUT_NAME=playwright-report.json playwright test",
    "security:lint": "node -e \"console.log('Security lint stub - use CodeQL/dep review in CI')\"",
    "security:headers:test": "jest --config jest.server.config.cjs --runInBand --testPathPattern=tests/server/security\\.headers\\.test\\.ts",
    "perf": "cross-env APP_URL=http://localhost:5000 node scripts/run-perf.cjs",
    "perf:k6:high": "k6 run tests/perf/api-highload.k6.js",
    "perf:k6": "k6 run tests/perf/basic.k6.js",
    "perf:verify": "cross-env APP_URL=http://localhost:5000 node scripts/perf-verify.cjs",
    "perf:artillery": "artillery run tests/perf/artillery.yml",
    "perf:node": "cross-env APP_URL=http://localhost:5000 SKIP_RATE_LIMIT=1 node scripts/perf-autocannon.cjs",
    "perf:accept": "cross-env APP_URL=http://localhost:5000 SKIP_RATE_LIMIT=1 node scripts/perf-node-suite.cjs",
    "perf:baseline": "cross-env APP_URL=http://localhost:5000 SKIP_RATE_LIMIT=1 node scripts/perf-save-baseline.cjs",
    "perf:regress": "cross-env APP_URL=http://localhost:5000 SKIP_RATE_LIMIT=1 node scripts/perf-compare.cjs",
    "perf:server": "cross-env NODE_ENV=test SKIP_RATE_LIMIT=1 PERF_MODE=1 tsx server/index.ts",
    "perf:server:prod": "npm run build && cross-env NODE_ENV=production SKIP_RATE_LIMIT=1 node dist/index.js",
    "perf:mem:long": "cross-env APP_URL=http://localhost:5000 SKIP_RATE_LIMIT=1 node tests/perf/memory-long.cjs",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "tsx scripts/run-migrations.js",
    "migrate": "node server/services/enhancedMigrationRunner.js",
    "migrate:clean": "node server/services/cleanMigrationRunner.js",
    "db:seed": "node scripts/seed-database.js",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "setup": "node run-creator-ai-studio.mjs setup",
    "app:start": "node run-creator-ai-studio.mjs start",
    "app:stop": "node run-creator-ai-studio.mjs stop",
    "app:restart": "node run-creator-ai-studio.mjs restart",
    "app:clean": "node run-creator-ai-studio.mjs clean",
    "app:status": "node run-creator-ai-studio.mjs status",
    "app:logs": "node run-creator-ai-studio.mjs logs --follow",
    "app:doctor": "node run-creator-ai-studio.mjs doctor",
    "migrate:enhanced": "node server/services/enhancedMigrationRunner.js",
    "migrate:safe": "node server/services/cleanMigrationRunner.js",
    "railway:start": "npm run start",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --external:vite",
    "build:simple": "vite build",
    "railway:build": "npm run build:simple",
    "verify:build": "node verify-build.cjs",
    "start:fallback": "node start-fallback.cjs"
  },
